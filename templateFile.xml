<!--

File used to list the headers and the template to use in case of
substitution

<header>Header column name:template to apply for this column</header>

The header name is a regex pattern

-->

<xml>

	<!--FXS ports-->
	<headers>
		<header>FORWARD .* CSS:forwardcss</header>
		<header>FORWARD .* CALLING SEARCH SPACE:forwardcss</header>
		<header>DEVICE POOL:devicepool</header>
		<header>.*CALLING SEARCH SPACE:devicecss</header>
		<header>DIRECTORY NUMBER:default</header>
		<header>ROUTE PARTITION:default</header>
		<header>LINE CSS:css</header>
		<header>FORWARD .* DESTINATION:default</header>
		<header>URGENT PRIORITY:urgentpriority</header>
	</headers>

