		<substitute>^(?!DP078|DP098).*$:*delete*</substitute>
		<substitute>DP078SRST:'078_SRST_DP'</substitute>
		<substitute>DP078:'078_DP'</substitute>
		<substitute>DP098SRST:'098_SRST_DP'</substitute>
		<substitute>DP098:'098_DP'</substitute>
	</template>
	<template>
		<name>devicecss</name>
		<substitute>.*:*Get Device Pool**5_**_3*+'_DEVICE_CSS'</substitute>
	</template>
	<template>
		<name>css</name>
		<substitute>\d{3}tout.*:'ALL_CSS'</substitute>
		<substitute>\d{3}international:'ALL_CSS'</substitute>
		<substitute>\d{3}interne:'INTERNAL_UK_CSS'</substitute>
		<substitute>\d{3}national.*:'NATGSMSPC_UK_CSS'</substitute>
